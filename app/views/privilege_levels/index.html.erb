<h1>Listing privilege_levels</h1>

  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

    <% @privilege_levels.each do |privilege_level| %>
      <tr>
        <td><%= privilege_level.name %></td>
        <td><%= link_to 'Show', privilege_level %></td>
        <td><%= link_to 'Edit', edit_privilege_level_path(privilege_level) %></td>
        <td><%= link_to 'Destroy', privilege_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>


